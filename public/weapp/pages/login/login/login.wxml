<scroll-view class="container" scroll-y="true" data-theme="{{themeStyle}}"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="iconfont iconclose back-btn" bindtap="__e"></view><view class="header-wrap"><view class="title">登录</view><block wx:if="{{registerConfig.is_enable==1}}"><view class="regisiter-agreement"><text class="ns-text-color-gray">还没有账号,</text><text data-event-opts="{{[['tap',[['toRegister',['$event']]]]]}}" class="ns-text-color" bindtap="__e">立即注册</text></view></block></view><view class="body-wrap"><view class="form-wrap"><view hidden="{{!(loginMode=='mobile')}}" class="input-wrap"><view class="content"><view class="area-code">+86</view><input class="input" type="number" placeholder="手机登录仅限中国大陆登录" placeholder-class="input-placeholder" maxlength="11" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['formData']]]]]}}" value="{{formData.mobile}}" bindinput="__e"/></view></view><view hidden="{{!(loginMode=='account')}}" class="input-wrap"><view class="content"><input class="input" type="text" placeholder="请输入账号" placeholder-class="input-placeholder" data-event-opts="{{[['input',[['__set_model',['$0','account','$event',[]],['formData']]]]]}}" value="{{formData.account}}" bindinput="__e"/></view></view><view hidden="{{!(loginMode=='account')}}" class="input-wrap"><view class="content"><input class="input" type="password" placeholder="请输入密码" placeholder-class="input-placeholder" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['formData']]]]]}}" value="{{formData.password}}" bindinput="__e"/><view hidden="{{!(loginMode=='account')}}" class="align-right"><text data-event-opts="{{[['tap',[['forgetPassword',['$event']]]]]}}" bindtap="__e">忘记密码?</text></view></view></view><view class="input-wrap"><view class="content"><input class="input" type="text" placeholder="请输入验证码" placeholder-class="input-placeholder" data-event-opts="{{[['input',[['__set_model',['$0','vercode','$event',[]],['formData']]]]]}}" value="{{formData.vercode}}" bindinput="__e"/><image class="captcha" src="{{captcha.img}}" mode data-event-opts="{{[['tap',[['getCaptcha',['$event']]]]]}}" bindtap="__e"></image></view></view><view hidden="{{!(loginMode=='mobile')}}" class="input-wrap"><view class="content"><input class="input" type="text" placeholder="请输入动态码" placeholder-class="input-placeholder" data-event-opts="{{[['input',[['__set_model',['$0','dynacode','$event',[]],['formData']]]]]}}" value="{{formData.dynacode}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['sendMobileCode',['$event']]]]]}}" class="{{['dynacode',dynacodeData.seconds==120?'ns-text-color':'ns-text-color-gray']}}" bindtap="__e">{{''+dynacodeData.codeText+''}}</view></view></view></view><view class="login-mode-box"><text data-event-opts="{{[['tap',[['switchLoginMode',['$event']]]]]}}" hidden="{{!(loginMode=='mobile')}}" bindtap="__e">使用账号登录</text><text data-event-opts="{{[['tap',[['switchLoginMode',['$event']]]]]}}" hidden="{{!(loginMode=='account'&&registerConfig.dynamic_code_login==1)}}" bindtap="__e">使用手机号登录</text></view><view class="btn_view"><button class="login-btn ns-border-color ns-bg-color" type="primary" data-event-opts="{{[['tap',[['login',['$event']]]]]}}" bindtap="__e">登录</button><block wx:if="{{isBind}}"><view data-event-opts="{{[['tap',[['authLogin',['$event']]]]]}}" class="auth-login ns-border-color" bindtap="__e"><text class="ns-text-color ns-border-color">一键授权登录</text></view></block><block wx:if="{{!isBind}}"><button class="auth-login ns-border-color" open-type="getPhoneNumber" data-event-opts="{{[['getphonenumber',[['mobileAuthLogin',['$event']]]]]}}" bindgetphonenumber="__e"><text class="ns-text-color ns-border-color">一键授权登录</text></button></block></view></view><ns-show-toast class="vue-ref" vue-id="5dad8992-1" title="{{title}}" data-ref="showToast" bind:__l="__l"></ns-show-toast></scroll-view>